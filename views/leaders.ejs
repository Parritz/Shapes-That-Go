<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Shapes that Lead</title>
</head>
<body>
    <table id="hiscore">
        <tr>
            <td rowspan="2">
                High Scores
            </td>
        </tr>
    </table>
    <a href="/">Back to Title Screen</a>
</body>
    <script>
        var leaders = <%- JSON.stringify(leaders) %>;
        console.log(leaders);
        let hiscore = document.getElementById("hiscore");
        for (const player in leaders) {
            let newRow = document.createElement('tr');
            let newPlayer = document.createElement('td');
            let newScore = document.createElement('td');
            newPlayer.innerText = Object.keys(leaders[player])[0];
            newScore.innerText = leaders[player][Object.keys(leaders[player])[0]]
            newRow.appendChild(newPlayer);
            newRow.appendChild(newScore);
            hiscore.appendChild(newRow);
        }
    </script>
</html>